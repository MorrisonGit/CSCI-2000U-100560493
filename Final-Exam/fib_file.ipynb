   "source": [
    "def fib_file(n, fname):\n",
    "    if(n<1):\n",
    "        exit()\n",
    "    \n",
    "    writer = open(fname, 'w')\n",
    "    for i in range(n):\n",
    "        i = i + (i-1) + (i-2)\n",
    "        writer.write(n, int)\n",
    "        writer.write(i, int)\n",
    "    writer.close()\n",
    "\n",
    "fib_file(6,'fib')"
   ]
